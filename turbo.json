{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {},
    "dev:web": {
      "cache": false,
      "persistent": true
    },
    "dev:ai": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "dependsOn": ["build"],
      "persistent": true
    },
    "start:web": {
      "cache": false,
      "persistent": true
    },
    "start:ai": {
      "cache": false,
      "persistent": true
    }
  }
}
